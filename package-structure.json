# Package.json for SMTOWN Marketplace
{
  "name": "smtown-marketplace",
  "version": "1.0.0",
  "description": "Local marketplace app for Ngọc Hồi, Kon Tum",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "eas build",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "submit": "eas submit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "generate-types": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > types/database.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "prebuild": "expo prebuild",
    "reset-project": "node ./scripts/reset-project.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-navigation/native": "^6.0.2",
    "@supabase/supabase-js": "^2.38.0",
    "expo": "~50.0.14",
    "expo-constants": "~15.4.5",
    "expo-font": "~11.10.3",
    "expo-image": "~1.10.6",
    "expo-image-picker": "~14.7.1",
    "expo-linking": "~6.2.2",
    "expo-location": "~16.5.5",
    "expo-notifications": "~0.27.6",
    "expo-router": "~3.4.8",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.3",
    "expo-web-browser": "~12.8.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^29.5.5",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.1",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.0.0",
    "jest": "^29.4.0",
    "jest-expo": "~50.0.4",
    "prettier": "^3.2.5",
    "react-test-renderer": "18.2.0",
    "supabase": "^1.148.6",
    "typescript": "^5.1.3"
  },
  "private": true,
  "keywords": [
    "marketplace",
    "react-native",
    "expo",
    "supabase",
    "vietnam",
    "kontum"
  ],
  "author": "SMTOWN Team",
  "license": "MIT"
}
